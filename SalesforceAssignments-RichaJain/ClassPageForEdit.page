<apex:page standardController="Class__c">
    <apex:form >
    	 <apex:pageBlock title="Class Edit Object" >
        	<apex:pageBlockSection >
                
                <apex:outputLabel value="Name"/>  <apex:inputField value="{!Class__c.Name}"/>
				<apex:outputLabel value="Class Teacher"/>  <apex:inputField value="{!Class__c.ClassTeacher__c}"/>
                <apex:outputLabel value="Max Size"/>  <apex:inputField value="{!Class__c.MaxSize__c}"/>
                <apex:outputLabel value="Fee"/>  <apex:inputField value="{!Class__c.Fee__c}"/>                      	
            </apex:pageBlockSection> 
             
            <apex:pageBlockButtons >
                	<apex:commandButton action="{!save}" Value="Save"/>
                	<apex:commandButton action="{!cancel}" Value="Cancel"/>
            </apex:pageBlockButtons> 
        </apex:pageBlock>                        
    </apex:form>
</apex:page>